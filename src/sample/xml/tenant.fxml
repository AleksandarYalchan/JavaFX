<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.controller.TenantController">
    <children>
        <TitledPane prefHeight="21.0" prefWidth="600.0" text="Create Agent"/>
        <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
                <Label text="Input Tenant Information"/>

                <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                    <children>
                        <Label text="First name:"/>
                        <TextField fx:id="firstNameTenant"/>
                    </children>
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                    </padding>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                    <children>
                        <Label text="Last name:"/>
                        <TextField fx:id="lastNameTenant"/>
                    </children>
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                    </padding>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                    <children>
                        <Label text="Rented Storage Units:"/>
                        <TextField fx:id="rentedStorageUnitsTenant"/>
                    </children>
                    <padding>
                        <Insets bottom="20.0" left="7.0" right="20.0" top="20.0"/>
                    </padding>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="30.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#createAgent" text="OK"/>
                        <Button mnemonicParsing="false" text="Clear"/>
                    </children>
                    <padding>
                        <Insets left="390.0" top="72.0"/>
                    </padding>
                </HBox>
            </children>
            <opaqueInsets>
                <Insets left="200.0"/>
            </opaqueInsets>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0"/>
            </padding>
        </VBox>
    </children>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
</VBox>
